{
  "name": "crystal-dating-app",
  "version": "0.1.0-prealpha",
  "description": "Revolutionary dating app built on transparency, equity, and meaningful connection",
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon src/server/index.js",
    "client": "cd src/client && npm start",
    "build": "cd src/client && npm run build",
    "generate-users": "node src/database/generateUsers.js",
    "test": "echo \"Tests coming soon\" && exit 0"
  },
  "keywords": [
    "dating",
    "matchmaking",
    "social",
    "transparency",
    "nonprofit"
  ],
  "author": "Crystal Dating Team",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "better-sqlite3": "^9.2.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}
